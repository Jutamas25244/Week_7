pipeline {
    agent any
    parameters {
        string(name: 'NAME', defaultValue: 'Jutamas', description: 'Enter your name')
        string(name: 'AGE', defaultValue: '21', description: 'Enter your age')
    }
    stages {
        stage('Example') {
            steps {
                echo "${params.NAME} --age ${params.AGE}"
                sh "python myscript.py Jutamas --age 21"
            }
        }
    }
}
